{
    "name": "vite-wallet",
    "version": "1.7.0",
    "main": "app/main.js",
    "description": "The Desktop app for vite.",
    "author": "Vitelabs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitelabs/vite-wallet.git"
    },
    "scripts": {
        "win:run:set:terminal:color": "call SetTerminalColorOn.bat",
        "win:run:check:terminal:color": "echo %FORCE_COLOR%",
        "win:run:set:platform": "call SetPlatformToWindows.bat",
        "dickbitch": "call SetPlatformToWindows.bat",
        "slut": "call SetPlatformToWindows",
        "crap": "call Crap.bat",
        "win:run:check:platform": "echo %platform%",
        "win:run:gulp": "yarn gulp",
        "win:run:copy": "xcopy vite-web-wallet\\dist\\* app\\walletPages\\ /E /I /H /Y",
        "clean": "rm -rf ./app/walletPages*",
        "copy:win": "xcopy vite-web-wallet\\dist\\* app\\walletPages\\ /E /I /H /Y",
        "build": "set FORCE_COLOR=1&&gulp&&set FORCE_COLOR=1&&set NODE_ENV=prod&&node build.js",
        "builddev": "gulp&&NODE_ENV=dev node build.js",
        "build:win": "set FORCE_COLOR=1&&set p=WIN&&echo %FORCE_COLOR%&&echo %p%&&npm run build",
        "build:web": "npm run build:web:test&&npm run build:web:prod",
        "build:web:prod": "cd vite-web-wallet&&npm run build&&cd ../ && cp -rf vite-web-wallet/dist/ app/walletPages",
        "build:web:test": "cd vite-web-wallet&&npm run testBuild&&cd ../ && cp -rf vite-web-wallet/dist/ app/walletPages-test",
        "dev": "set NO_BUILD=true&&npm run builddev&&set NODE_ENV=dev&&set HOT_RELOAD=true&&electron .",
        "dev:nohot": "set NO_BUILD=true&&npm run builddev&&set NODE_ENV=dev&&electron .",
        "test": "set FORCE_COLOR=1&&set p=WIN&&echo %FORCE_COLOR%&&echo %p%",
        "lint": "eslint ./ --cache",
        "devd": "gulp",
        "release:mac": "set FORCE_COLOR=1&&npm run build",
        "release:win": "set FORCE_COLOR=1&&npm run build:win",
        "release": "set FORCE_COLOR=1&&node release.js",
        "commit": "cz",
        "blowup": "yarn run blowup",
        "dipshit": "yarn run asshole",
        "asshole": "yarn run dipshit",
        "test2": "set FORCE_COLOR=1&&set p=WIN&&call echo %FORCE_COLOR%&&call echo %p%",
        "test3": "yarn gulp",
        "test4": "set FORCE_COLOR=1&&yarn gulp&&call echo %FORCE_COLOR%",
        "test5": "yarn run run:gulp",
        "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "babel-eslint": "^10.1.0",
        "babel-preset-es2016-node5": "^1.1.2",
        "commitizen": "^4.2.2",
        "cz-conventional-changelog": "3.3.0",
        "dotenv": "^8.2.0",
        "electron": "^11.1.1",
        "electron-builder": "^22.9.1",
        "electron-notarize": "^1.0.0",
        "eslint": "~7.12.1",
        "eslint-plugin-vue": "~7.1.0",
        "event-pubsub": "^5.0.3",
        "execa": "^5.0.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "inquirer": "^7.3.3",
        "js-queue": "^2.0.0",
        "mocha": "^8.2.0",
        "shelljs": "^0.8.4"
    },
    "dependencies": {
        "@trodi/electron-splashscreen": "^1.0.0",
        "auto-launch": "^5.0.5",
        "electron-log": "^4.2.4",
        "electron-serve": "^1.0.0",
        "electron-store": "^6.0.1",
        "electron-updater": "^6.3.9",
        "moment": "^2.29.1",
        "nvexeca": "^6.0.1",
        "semver": "^7.3.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
